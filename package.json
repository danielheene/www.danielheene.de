{
  "name": "daniel.heene.io",
  "version": "0.1.1",
  "private": true,
  "description": "Personal Website for daniel.heene.io",
  "repository": "https://github.com/danielheene/danielheene.git",
  "license": "MIT",
  "author": "Daniel Heene <daniel@heene.io>",
  "workspaces": [
    "website",
    "studio"
  ],
  "scripts": {
    "chore": "run-s \"chore:**\" --print-label",
    "chore:deduplicate": "yarn deduplicate",
    "chore:fix-mismatches": "syncpack fix-mismatches",
    "chore:set-versions": "set-versions --workspaces",
    "dev": "dotenv -- turbo run dev --include-dependencies --parallel --continue",
    "build": "dotenv -- turbo run build --include-dependencies",
    "env": "DOPPLER_CONFIG=development_local doppler secrets download --format env --no-file > .env || true",
    "sanity": "yarn dotenv -- yarn workspace studio sanity"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/prettier": "^2.7.1",
    "dotenv-cli": "6.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "set-versions": "1.0.3",
    "sort-package-json": "2.0.0",
    "syncpack": "8.2.4",
    "turbo": "1.5.5",
    "typescript": "^4.8.4"
  },
  "packageManager": "yarn@3.2.3"
}
