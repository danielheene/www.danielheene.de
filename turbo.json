{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "dependsOn": [
        "^dev"
      ]
    },
    "website#build": {
      "dependsOn": [
        "studio#build"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "cache": false,
      "outputs": [
        "./frontend/.next",
        "./frontend/public/studio"
      ]
    }
  },
  "globalEnv": [
    "BASE_URL",
    "SANITY_STUDIO_DATASET",
    "SANITY_STUDIO_PROJECT_ID",
    "SANITY_STUDIO_PROJECT_BASEPATH",
    "SANITY_STUDIO_API_VERSION",
    "SANITY_STUDIO_API_TOKEN"
  ]
}


